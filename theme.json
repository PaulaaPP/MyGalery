{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 2,
  "settings": {
    "layout": {
      "contentSize": "1183px",
      "wideSize": "1183px"
    },
    "color": {
      "palette": [
        {
          "slug": "green-galery",
          "color": "#499166",
          "name": "Green"
        },
        {
          "slug": "light-button",
          "color": "#97928B",
          "name": "greenbutton"
        },
        {
          "slug": "blue-button",
          "color": "#3B5362",
          "name": "Blue-sky"
        },
        {
          "slug": "grey-button",
          "color": "#616D70",
          "name": "Blue-sky"
        }
      ]
    },
    "typography": {
      "fontFamilies": [
        {
          "fontFamily": "'Inter', sans-serif",
          "slug": "inter",
          "name": "Body - Inter",
          "fontFace": [
            {
              "fontFamily": "'Inter', sans-serif",
              "fontWeight": "300",
              "fontStyle": "normal",
              "fontStretch": "normal",
              "src": [
                "https://fonts.googleapis.com/css2?family=Inter:wght@300;400&display=swap"
              ]
            }
          ]
        },
        {
          "fontFamily": "'Gentium Book Plus', serif",
          "slug": "Gentium",
          "name": "Heading - Gentium"
        }
      ],
      "fontSizes": [
        {
          "slug": "small",
          "size": "0.64rem",
          "name": "small"
        },
        {
          "slug": "medium",
          "size": "0.8rem",
          "name": "medium"
        },

        {
          "slug": "regular",
          "size": "1rem",
          "name": "regular"
        }
      ]
    },
    "custom": {
      "typography": {
        "fontSize": {
          "heading-1": "clamp(2.113rem, 1.442rem + 1.789vw, 3.052rem)",
          "heading-2": "clamp(1.699rem, 1.5506000000000002rem + 0.7419999999999998vw, 2.441rem)",
          "heading-3": "clamp(1.35rem, 1.2294rem + 0.603vw, 1.953rem)"
        }
      },
      "line-height": {
        "tiny": 1.15,
        "small": 1.3,
        "medium": 1.4,
        "normal": 1.6
      }
    },
    "spacing": {
      "padding": true,
      "margin": true,
      "units": ["px", "em", "rem", "vh", "vw", "%"]
    },
    "border": {
      "radius": true,
      "color": true,
      "style": true,
      "width": true
    }
  },
  "styles": {
    "color": {
      "background": "white",
      "text": "black"
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--inter)",
      "lineHeight": "1.75",
      "fontSize": "1rem"
    },
    "elements": {
      "h1": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--gentium)",
          "fontWeight": "700",
          "lineHeight": "var(--wp--custom--line-height--small)",
          "fontSize": "var(--wp--custom--typography--font-size--heading-1)"
        },
        "spacing": {
          "margin": {
            "top": "20px",
            "bottom": "20px",
            "left": "0",
            "right": "0"
          }
        }
      },
      "h2": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--gentium)",
          "fontWeight": "700",
          "lineHeight": "var(--wp--custom--line-height--small)",
          "fontSize": "var(--wp--custom--typography--font-size--heading-2)"
        },
        "spacing": {
          "margin": {
            "top": "20px",
            "bottom": "20px",
            "left": "0",
            "right": "0"
          }
        }
      },
      "h3": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--gentium)",
          "fontWeight": "700",
          "lineHeight": "var(--wp--custom--line-height--small)",
          "fontSize": "var(--wp--custom--typography--font-size--heading-3)"
        },
        "spacing": {
          "margin": {
            "top": "20px",
            "bottom": "20px",
            "left": "0",
            "right": "0"
          }
        }
      },
      "link": {
        "color": {
          "text": "currentColor"
        }
      }
    },
    "blocks": {
      "core/button": {
        "border": {
          "color": "currentColor",
          "radius": "5px"
        },
        "color": {
          "background": "#499166",
          "text": "black"
        },
        "spacing": {
          "padding": {
            "top": "0.7em",
            "right": "1.6em",
            "bottom": "0.7em",
            "left": "1.6em"
          }
        },
        "typography": {
          "fontSize": "va(--wp--prset--font-size--regular)",
          "lineHeight": "var(--wp--custom--line-height--normal)"
        }
      }
    }
  },
  "templateParts": [
    {
      "name": "header",
      "area": "header",
      "title": "Header"
    },
    {
      "name": "footer",
      "area": "footer",
      "title": "Footer"
    }
  ]
}
